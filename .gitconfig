[remote "kakmt"]
    url = git@github.com:kakmt/arch-docker-container.git
    fetch = +refs/heads/*:refs/remotes/kakmt/*
[remote "origin"]
    url = git@github.com:huangyingw/kakmt_arch-docker-container.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[push]
    remote = origin
[gsync]
    remote = kakmt
    target = kakmt/master
[merge]
    allow = true
[branch "dev"]
    remote = origin
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[gdif]
    ignore = *.gdio.diff
[deploy]
	host = localhost
	path = ~/myproject/git/docker/kakmt/arch-docker-container
